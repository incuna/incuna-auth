#: incuna_auth/middleware/basic_auth.py:9
msgid "Restricted Access"
msgstr "Korlátozott hozzáférés"

#: incuna_auth/middleware/permission.py:115
msgid "You must be logged in to view this page."
msgstr "Az oldal megtekintéséhez be kell jelentkeznie."

#: incuna_auth/templates/registration/base/login_base.html:6
#: incuna_auth/templates/registration/base/login_base.html:8
msgid "Log In"
msgstr "Bejelentkezés"

#: incuna_auth/templates/registration/base/login_form_base.html:7
msgid "Please log in using your email and password."
msgstr "Kérjük, jelentkezzen be az e-mail-címével és a jelszavával."

#: incuna_auth/templates/registration/base/login_form_base.html:16
msgid "Forgotten password?"
msgstr "Elfelejtette jelszavát?"

#: incuna_auth/templates/registration/base/login_form_base.html:22
msgid "Log in"
msgstr "Bejelentkezés"

#: incuna_auth/templates/registration/base/logout_base.html:6
#: incuna_auth/templates/registration/base/logout_base.html:8
msgid "Logged Out"
msgstr "Kijelentkezve"

#: incuna_auth/templates/registration/base/logout_base.html:11
msgid "You have logged out."
msgstr "Ön kijelentkezett."

#: incuna_auth/templates/registration/base/password_change_done_base.html:7
#: incuna_auth/templates/registration/base/password_change_done_base.html:9
msgid "Password Change Successful"
msgstr "A jelszó megváltoztatása sikeres"

#: incuna_auth/templates/registration/base/password_change_done_base.html:12
msgid "Your password was changed."
msgstr "Jelszava megváltozott."

#: incuna_auth/templates/registration/base/password_change_form_base.html:8
#: incuna_auth/templates/registration/base/password_change_form_base.html:10
msgid "Password Change"
msgstr "Jelszó módosítása"

#: incuna_auth/templates/registration/base/password_change_form_base.html:13
msgid ""
"Please enter your old password, for security's sake, and then enter your new "
"password twice so we can verify you typed it in correctly."
msgstr ""
"Kérjük, biztonsági okokból adja meg a régi jelszavát, majd adja meg kétszer "
"az új jelszót annak érdekében, hogy le tudjuk ellenőrizni, hogy helyesen "
"gépelte-e be."

#: incuna_auth/templates/registration/base/password_change_form_base.html:26
#: incuna_auth/templates/registration/base/password_reset_confirm_base.html:39
msgid "Change my password"
msgstr "Jelszó megváltoztatása"

#: incuna_auth/templates/registration/base/password_reset_complete_base.html:6
#: incuna_auth/templates/registration/base/password_reset_complete_base.html:8
msgid "Password Reset Complete"
msgstr "Jelszó visszaállítása befejeződött"

#: incuna_auth/templates/registration/base/password_reset_complete_base.html:11
#, python-format
msgid ""
"Your password has been set. You may go ahead and <a href=\"%(login_url)s"
"\">log in</a> now."
msgstr ""
"Jelszava beállítása befejeződött. Folytathatja, és most már <a href="
"\"%(login_url)s\">bejelentkezhet</a>."

#: incuna_auth/templates/registration/base/password_reset_confirm_base.html:10
#: incuna_auth/templates/registration/base/password_reset_confirm_base.html:18
msgid "Enter New Password"
msgstr "Adjon meg új jelszót"

#: incuna_auth/templates/registration/base/password_reset_confirm_base.html:12
#: incuna_auth/templates/registration/base/password_reset_confirm_base.html:20
msgid "Password Reset Unsuccessful"
msgstr "A jelszó visszaállítása sikertelen"

#: incuna_auth/templates/registration/base/password_reset_confirm_base.html:26
msgid ""
"Please enter your new password twice so we can verify you typed it in "
"correctly."
msgstr ""
"Kérjük, adja meg kétszer az új jelszót annak érdekében, hogy le tudjuk "
"ellenőrizni, hogy helyesen gépelte-e be."

#: incuna_auth/templates/registration/base/password_reset_confirm_base.html:46
msgid ""
"The password reset link was invalid, possibly because it has already been "
"used."
msgstr ""
"A jelszó-visszaállítási hivatkozás érvénytelen, valószínűleg azért, mert már "
"felhasználta."

#: incuna_auth/templates/registration/base/password_reset_confirm_base.html:50
#, python-format
msgid ""
"Please <a href=\"%(password_reset_url)s\">request a new password</a> reset."
msgstr ""
"Kérjük, <a href=\"%(password_reset_url)s\">kérjen új jelszó-visszaállítást</"
"a>."

#: incuna_auth/templates/registration/base/password_reset_confirm_base.html:52
msgid "Please request a new password reset."
msgstr "Kérjük, kérjen új jelszó visszaállítást."

#: incuna_auth/templates/registration/base/password_reset_done_base.html:6
#: incuna_auth/templates/registration/base/password_reset_done_base.html:8
msgid "Password Reset Successful"
msgstr "A jelszó visszaállítása sikeres"

#: incuna_auth/templates/registration/base/password_reset_done_base.html:11
msgid ""
"We've e-mailed you instructions for setting your password to the e-mail "
"address you submitted. You should be receiving it shortly."
msgstr ""
"Elküldtük e-mailben a jelszó beállításához szükséges utasításokat az Ön "
"által megadott e-mail-címre. Rövidesen meg kell kapnia."

#: incuna_auth/templates/registration/base/password_reset_email_base.html:8
msgid "You're receiving this e-mail because you requested a password reset"
msgstr "Ön azért kapta ezt az e-mailt, mert jelszó-visszaállítást kért"

#: incuna_auth/templates/registration/base/password_reset_email_base.html:9
#, python-format
msgid "for your user account at %(site_name)s"
msgstr "a következő webhelyen meglévő felhasználói fiókjához: %(site_name)s"

#: incuna_auth/templates/registration/base/password_reset_email_base.html:11
msgid "Please go to the following page and choose a new password:"
msgstr "Kérjük, menjen a következő oldalra, és adjon meg egy új jelszót."

#: incuna_auth/templates/registration/base/password_reset_email_base.html:15
msgid "Your username, in case you've forgotten:"
msgstr "Ha esetleg elfelejtette volna, felhasználóneve a következő:"

#: incuna_auth/templates/registration/base/password_reset_email_base.html:17
msgid "Thanks for using our site!"
msgstr "Köszönjük, hogy használta a webhelyünket!"

#: incuna_auth/templates/registration/base/password_reset_email_base.html:19
#, python-format
msgid "The %(site_name)s team"
msgstr "A(z) %(site_name)s csapata"

#: incuna_auth/templates/registration/base/password_reset_form_base.html:7
#: incuna_auth/templates/registration/base/password_reset_form_base.html:9
msgid "Password Reset"
msgstr "Jelszó visszaállítása"

#: incuna_auth/templates/registration/base/password_reset_form_base.html:12
msgid ""
"Forgotten your password? Enter your e-mail address below, and we'll e-mail "
"instructions for setting a new one."
msgstr ""
"Elfelejtette a jelszavát? Adja meg alább az e-mail-címét, és elküldjük Önnek "
"az új jelszó létrehozásával kapcsolatos utasításokat."

#: incuna_auth/templates/registration/base/password_reset_form_base.html:25
msgid "Reset"
msgstr "Visszaállítás"

#: incuna_auth/urls.py:44
msgid "^login/$"
msgstr "^login/$"

#: incuna_auth/urls.py:50
msgid "^logout/$"
msgstr "^logout/$"

#: incuna_auth/urls.py:57
msgid "^password/change/$"
msgstr "^password/change/$"

#: incuna_auth/urls.py:62
msgid "^password/change/done/$"
msgstr "^password/change/done/$"

#: incuna_auth/urls.py:68
msgid "^password/reset/$"
msgstr "^password/reset/$"

#: incuna_auth/urls.py:74
msgid "^password/reset/done/$"
msgstr "^password/reset/done/$"

#: incuna_auth/urls.py:79
msgid "^password/reset/complete/$"
msgstr "^password/reset/complete/$"

#: incuna_auth/urls.py:84
msgid "^sso/$"
msgstr "^sso/$"

#: incuna_auth/urls.py:94
msgid ""
"^password/reset/confirm/(?P<uidb64>[0-9A-Za-z_\\-]+)/(?P<token>[0-9A-Za-z]"
"{1,13}-[0-9A-Za-z]{1,20})/$"
msgstr ""
"^password/reset/confirm/(?P<uidb64>[0-9A-Za-z_\\-]+)/(?P<token>[0-9A-Za-z]"
"{1,13}-[0-9A-Za-z]{1,20})/$"

#: incuna_auth/urls.py:107
msgid ""
"^password/reset/confirm/(?P<uidb36>[0-9A-Za-z]{1,13})-(?P<token>[0-9A-Za-z]"
"{1,13}-[0-9A-Za-z]{1,20})/$"
msgstr ""
"^password/reset/confirm/(?P<uidb36>[0-9A-Za-z]{1,13})-(?P<token>[0-9A-Za-z]"
"{1,13}-[0-9A-Za-z]{1,20})/$"
